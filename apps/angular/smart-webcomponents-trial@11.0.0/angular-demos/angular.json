{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "bfb2ba34-e0c2-4f8d-a0f5-da452a8ffc9d"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "bargauge": {
      "projectType": "application",
      "root": "",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "assets": [],
            "baseHref": "/",
            "outputPath": "dist/grid/overview",
            "index": "demos/grid/overview/index.htm",
            "main": "src/grid/overview/main.ts",
            "polyfills": "polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "styles": [
              "node_modules/smart-webcomponents-angular/source/styles/smart.default.css"
            ]
          },
          "configurations": {
            "production": {
              "optimization": false,
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": false,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "bargauge:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "bargauge:build:production"
            },
            "staging": {
              "browserTarget": "bargauge:build:staging"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "codeCoverage": true,
            "main": "test.ts",
            "polyfills": "polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              {
                "glob": "**/*",
                "input": "node_modules/smart-webcomponents-angular/source",
                "output": "src/test/assets"
              },
              "src/test/assets"
            ],
            "styles": [
              "node_modules/smart-webcomponents-angular/source/styles/smart.default.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}